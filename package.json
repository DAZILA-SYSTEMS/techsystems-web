		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"electron": "^23.2.0",
		"electron-builder": "^23.6.0"
	},
	"author": {
		"name": "Techsystems Group Limited",
		"email": "care@techsystems.best",
		"url": "https://techsystems.best"
	},
	"build": {
		"appId": "techsystems.school",
		"productName": "School Techsystem",
		"copyright": "Copyright Â© 2023 ${author}",
		"mac": {
			"category": "public.app-category.utilities"
		},
		"win": {
			"target": [
				{
					"target": "nsis",
					"arch": [
						"ia32"
					]
				}
			]
		},
		"files": [
			"build/**/*",
			"node_modules/**/*"
